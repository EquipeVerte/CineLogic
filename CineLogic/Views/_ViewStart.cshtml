@{
    if(Session[CineLogic.Models.Libraries.SessionTypes.type] != null)
    {
        if (Session[CineLogic.Models.Libraries.SessionTypes.type].Equals(CineLogic.Models.Libraries.UserTypes.admin))
        {
            Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
        }
        else
        {
            Layout = "~/Views/Shared/_Layout.cshtml";
        }
    }
    else
    {
        Layout = "~/Views/Shared/_LayoutAccueil.cshtml";
    }
}
